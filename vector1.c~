
#include <iostream>
#include <vector>
using namespace std;
void print( const vector<int> ) ;//utility function outputs a vector of ints

/*Then we describe the main program: */
int main()
{
	vector<int> v;
	int number;

        int i=10;
	while(i--){
                cin>>number;
		v.push_back(number);
	}

	print(v);

}

void print( const vector<int> a)
{
	for(int i=0; i<a.size(); ++i)
		cout << a[i] << " ";

	cout << endl<<"----------------"<<endl;

}


